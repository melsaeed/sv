<?xml version='1.0' encoding='utf-8'?>
<widget id="com.safavisa.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Safavisa</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="m.elsaeed@brightery.com" href="http://brightery.com">
        Muhammad El-Saeed
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <access origin="*" />
    <allow-navigation href="*" subdomains="true" />
    <access launch-external="yes" origin="*" />
    <allow-intent href="*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="websecurity" value="disable" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
</widget>
